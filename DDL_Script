# 사용자
CREATE TABLE user (
    user_id BIGINT AUTO_INCREMENT NOT NULL PRIMARY KEY COMMENT '사용자ID',
    social_type VARCHAR(30) COMMENT 'SNS유형',
    social_id VARCHAR(255) COMMENT 'SNS아이디',
    email VARCHAR(255) COMMENT '이메일',
    name VARCHAR(50) COMMENT '이름',
    nickname VARCHAR(255) COMMENT '별명',
    age INT COMMENT '나이',
    city VARCHAR(255) COMMENT '지역',
    image_url VARCHAR(255) COMMENT '이미지주소',
    phone_number VARCHAR(255) COMMENT '휴대폰번호',
    role varchar(30) COMMENT '역할',
    password VARCHAR(255) COMMENT '비밀번호',
    refresh_token VARCHAR(255) COMMENT '리프레시토큰',
    is_mkt_agr BOOLEAN DEFAULT FALSE COMMENT '마케팅동의여부',
    is_withdraw BOOLEAN DEFAULT FALSE COMMENT '탈퇴여부',
    create_at TIMESTAMP NOT NULL COMMENT '생성일',
    update_at TIMESTAMP NOT NULL COMMENT '수정일'
);

# 주택
CREATE TABLE house (
    house_id BIGINT AUTO_INCREMENT NOT NULL PRIMARY KEY COMMENT '주택ID',
    user_id BIGINT NOT NULL COMMENT '사용자ID',
    house_type CHAR(1) NOT NULL COMMENT '주택유형',
    house_name VARCHAR(100) NOT NULL COMMENT '주택명',
    detail_adr VARCHAR(255) COMMENT '상세주소',
    contract_date DATE COMMENT '계약일자',
    balance_date DATE COMMENT '잔금지급일자',
    buy_date DATE COMMENT '취득일자',
    move_in_date DATE COMMENT '전입일자',
    move_out_date DATE COMMENT '전출일자',
    sell_date DATE COMMENT '양도일자',
    buy_price BIGINT COMMENT '취득금액',
    sell_price BIGINT COMMENT '양도금액',
    pub_land_price BIGINT COMMENT '공시지가',
    kb_price BIGINT COMMENT 'KB시세',
    jibun_addr VARCHAR(255) COMMENT '지번주소',
    road_addr VARCHAR(255) COMMENT '도로명주소',
    road_adr_ref VARCHAR(255) COMMENT '도로명주소참고항목',
    bd_mgt_sn CHAR(25) COMMENT '건물관리번호',
    adm_cd CHAR(10) COMMENT '행정구역코드',
    rn_mgt_sn CHAR(12) COMMENT '도로명코드',
    area DECIMAL(7,3) COMMENT '전용면적',
    is_destruction BOOLEAN DEFAULT FALSE COMMENT '멸실여부',
    is_cur_own BOOLEAN DEFAULT TRUE COMMENT '현재소유여부',
    owner_cnt INT COMMENT '소유자수',
    userProportion INT COMMENT '본인지분비율',
    is_move_in_right BOOLEAN DEFAULT FALSE COMMENT '입주권여부',
    source_type VARCHAR(30) COMMENT '출처유형',
    create_at TIMESTAMP NOT NULL COMMENT '생성일',
    update_at TIMESTAMP NOT NULL COMMENT '수정일'
);